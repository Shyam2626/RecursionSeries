import java.util.*;

public class Main {
  
    private static int factorial(int n){
      if(n==1) return 1;
      if(n==0) return 0;
      return n*factorial(n-1);
    }
    
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      System.out.println(factorial(sc.nextInt()));
  }
}
